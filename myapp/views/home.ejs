    <title>Binder - Get Connected</title>
</head>
<body>
    <% include partials/header.ejs %>
    <% include partials/navbar.ejs %>
    <% include partials/modal.ejs %>
    <div class="container">
        <h1>Get Student Information</h1>
        <form action="">
            <div class="form-group">
                <label for="majorName">Major</label>
                <input type="text" class="form-control" id="majorName" name="majorName" placeholder="Major">
            </div>
            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="complex">Apartment Complex</label>
                    <input type="input" class="form-control" id="complex" placeholder="Apartment Complex">
                </div>
                <div class="form-group col-md-6">
                    <label for="apartment">Apartment Number</label>
                    <input type="input" class="form-control" id="apartment" placeholder="Apartment Number">
                </div>
            </div>
            <button class="btn btn-primary" type="button" onClick="getInfo()">Submit</button>
        </form>
    </div>
    <% include partials/footer.ejs %>
    </body>
